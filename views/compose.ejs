<%- include("partials/header"); -%>
    <h1>Compose</h1>
    <form action="/compose" method="post">
        <div class="form-group">
          <label>Title</label>
          <input class="form-control" name="post_title" autocomplete="off">
        </div>
        <div class="form-group">
            <label>Post</label>
            <textarea class="form-control" rows="7" name="post_content" autocomplete="off"></textarea>
        </div>
        <button type="submit" class="btn btn-primary">Publish</button>
    </form>
<%- include("partials/footer"); -%>
